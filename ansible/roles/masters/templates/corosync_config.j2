totem {
    version: 2
    secauth: off
    cluster_name: {{ pcs_cluster_name }}
    transport: udpu
}

nodelist {
    node {
        ring0_addr: {{ pcs_master1 }}
        nodeid: 1
    }

    node {
        ring0_addr: {{ pcs_master2 }}
        nodeid: 2
    }
}

quorum {
    provider: corosync_votequorum
    two_node: 1
}

logging {
    to_logfile: yes
    logfile: /var/log/cluster/corosync.log
    to_syslog: yes
}
